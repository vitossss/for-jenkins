#!groovy 
// Run docker build
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    stages {
        stage('Build a docker image') {
            steps {
                sh 'docker build -t hello .' 
            }
        }
        stage('Run docker image') {
            steps {
                sh 'docket run --rm hello'
            }
        }
    }
}
